generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model fancy_chess_test {
  id String @id @default(auto()) @map("_id") @db.ObjectId

  @@map("fancy-chess-test")
}

model fancy_chess {

    id    String @id @default(auto()) @map("_id") @db.ObjectId
    gameID    String
    board     String
    createdAt DateTime @default(now())
    updatedAt DateTime @updatedAt
}

